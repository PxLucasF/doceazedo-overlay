{
  "name": "doceazedo-overlay",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "FORCE_COLOR=1 turbo run dev --parallel",
    "start": "FORCE_COLOR=1 turbo run start",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "cmd:new": "vite-node scripts/new-command.ts --"
  },
  "devDependencies": {
    "turbo": "latest",
    "vite-node": "^0.31.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "packageManager": "npm@8.18.0"
}
