{
  "name": "doceazedo-overlay",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "data/*",
    "packages/*"
  ],
  "scripts": {
    "build": "(cd apps/overlay && npm run build) && npm run start",
    "dev": "turbo run dev --parallel --no-cache --force",
    "start": "turbo run start --no-cache --force",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "postinstall": "vite-node scripts/postinstall.ts"
  },
  "devDependencies": {
    "turbo": "latest",
    "vite-node": "^0.31.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "packageManager": "npm@8.18.0",
  "dependencies": {
    "dotenv-cli": "^7.2.1",
    "vm2": "^3.9.19"
  }
}
